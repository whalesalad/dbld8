# Ignore bundler config
.DS_Store
/.bundle
/.env
/.foreman

# Ignore all logfiles and tempfiles.
/log/*
/tmp

/public/assets
/public/system
.sass-cache/

# Ignore Solr data and process ident files
/solr/data
/solr/pids
